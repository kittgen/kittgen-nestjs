{
  "name": "@kittgen/nestjs-https-redirect",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Ottgar",
  "private": false,
  "description": "Kittgen HTTPS redirect middleware for NestJS.",
  "keywords": [
    "https",
    "redirect",
    "middleware",
    "security",
    "library",
    "nest",
    "nestjs"
  ],
  "contributors": [
    "Otto von Wesendonk",
    "Edgar Mueller"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kittgen/kittgen-nestjs.git"
  },
  "bugs": {
    "url": "https://github.com/kittgen/kittgen-nestjs/issues"
  },
  "main": "dist/index.js",
  "module": "dist/https-redirect.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "tsdx lint",
    "semantic-release": "semantic-release",
    "upgrade-interactive": "npx npm-check --update"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "tsdx lint src --fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@golevelup/nestjs-testing": "^0.1.2",
    "@nestjs/testing": "^7.6.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.9",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^24.9.1",
    "husky": "^3.0.9",
    "jest": "^26.6.3",
    "semantic-release": "^17.3.1",
    "ts-jest": "^26.4.4",
    "tsdx": "^0.14.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.4",
    "@nestjs/core": "^7.6.5",
    "@nestjs/platform-express": "^7.6.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "preset": "ts-jest/presets/js-with-ts",
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
